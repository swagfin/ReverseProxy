{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "PartitioningLoadbalancer.LoadbalancerPolicy": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "LoadBalancingPolicy": "PartitioningByIPAddress", //RoundRobin,PowerOfTwoChoices,Random,First,LeastRequests >> PartitioningByIPAddress
        "Destinations": {
          "cluster1/destination1": {
            //  "Address": "http://localhost:8080/path1/"
            "Address": "https://endmz6tagrnzb.x.pipedream.net/path1"
          },
          "cluster1/destination2": {
            // "Address": "http://localhost:8080/path2/",
            "Address": "https://endmz6tagrnzb.x.pipedream.net/path2"
          },
          "cluster1/destination3": {
            // "Address": "http://localhost:8080/path3/"
            "Address": "https://google.co.ke"
          },
          "cluster1/destination4": {
            //"Address": "http://localhost:8080/path4/"
            "Address": "https://uonbi.ac.ke"
          }
        }
      }
    }
  }
}
